#include <stdio.h>
#include <string.h>

int main(int argc, char *argv[])
{
const char *originalProgramName = argv[0];
const char *programName = strrchr(originalProgramName, '/');
if (programName != NULL) {
programName++;
} else {
programName = originalProgramName;
}
printf("Program name: %s\n", programName);
return (0);
}
